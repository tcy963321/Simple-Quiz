    "class Square:\n",
    "    def __init__(self, side):\n",
    "        self.side = side\n",
    "    \n",
    "    def setSide(self):\n",
    "        if self.side < 0:\n",
    "            self.side = 0\n",
    "        else:\n",
    "            print(self.side * self.side)\n",
    "square = Square(3)\n",
    "square.setSide()"
